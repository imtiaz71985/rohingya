	<queryString>
		<![CDATA[SELECT ed.id ,ed.version,CASE WHEN edi.is_additional=1 THEN'Sector/CSU Specific Issues' ELSE edi.issue_name END AS issueName
,CASE WHEN edi.is_additional=1 THEN 7 ELSE edi.id END AS issueId ,DATE_FORMAT(month_for, '%M %Y') MONTH,
       DATE_FORMAT(status_change_date, '%M %Y') resolvedMonth,ed.description,ed.remarks,ed.ed_advice AS edAdvice
       FROM  ed_dashboard_issues edi
       RIGHT JOIN ed_dashboard ed ON ed.issue_id=edi.id AND ed.service_id = $P{serviceId}
       WHERE  service_id = $P{serviceId} AND is_resolve = TRUE
        ORDER BY edi.id]]>
